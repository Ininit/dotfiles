# tmux & tmuxinator
alias tn 'tmux new-session -s'
alias tk 'tmux kill-session -t'
alias tw 'tmux switch -t'
alias bye 'tmux kill-server'

# dotfiles
alias dot 'cd $DOTFILES'

# neovim
alias vim 'nvim'

# git
alias gst 'git status'
alias gch 'git checkout'
alias gca 'git commit --amend'

# better command
alias cat  'bat'
alias find 'fd'